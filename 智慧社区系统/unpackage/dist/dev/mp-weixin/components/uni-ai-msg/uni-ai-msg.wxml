<block wx:if="{{!msg.isDelete}}"><view class="msg-item"><view class="create_time-box"><uni-dateformat class="create_time" vue-id="100a993c-1" date="{{msg.create_time}}" format="MM/dd hh:mm:ss" bind:__l="__l"></uni-dateformat></view><view class="{{[(!msg.isAi)?'reverse':'']}}"><view class="userInfo"><image class="avatar" src="{{msg.isAi?headimg:userinfoObj.trans_pic}}" mode="widthFix"></image></view><view class="content"><block wx:if="{{msg.isAi}}"><view data-ref="rich-text-box" class="{{['rich-text-box','vue-ref',(showCursor)?'show-cursor':'']}}"><block wx:if="{{$root.g0}}"><rich-text space="nbsp" nodes="{{nodes}}" data-event-opts="{{[['itemclick',[['trOnclick',['$event']]]]]}}" binditemclick="__e"></rich-text></block></view></block><block wx:else><view>{{msgContent}}</view></block><view class="{{['menu-box',(msg.isAi)?'menu-box-ai':'']}}"><block wx:if="{{isLastMsg&&msg.isAi}}"><uni-icons class="pointer change-answer" vue-id="100a993c-2" title="换一个答案" color="#ccc" type="reload" size="16" data-event-opts="{{[['^click',[['changeAnswer']]]]}}" bind:click="__e" bind:__l="__l"></uni-icons></block><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="more-icon-box" bindtap="__e"><uni-icons class="more-icon pointer" vue-id="100a993c-3" size="12px" type="more-filled" color="#ccc" bind:__l="__l"></uni-icons></view><block wx:if="{{showMoreMenu}}"><view class="more-menu"><view class="copy-icon pointer" title="复制" data-event-opts="{{[['tap',[['copy',['$event']]]]]}}" bindtap="__e"><view class="copy-icon-a"></view><view class="copy-icon-b"></view></view><uni-icons class="remove-msg pointer" vue-id="100a993c-4" type="trash" size="20" color="#ccc" data-event-opts="{{[['^click',[['removeMsg']]]]}}" bind:click="__e" bind:__l="__l"></uni-icons></view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="more-menu-mask" bindtap="__e"></view></block></view></view></view></view></block>